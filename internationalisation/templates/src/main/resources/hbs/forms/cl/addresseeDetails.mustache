<fieldset class="CoverLetter.Addressee.ContactInfo">

	<fieldset class="CoverLetter.Addressee.CompanyName" name="CompanyName">
		<label for="Email">[[#resources]][[& GuiLabel_CoverLetter:Addressee:Organisation]][[/resources]]</label>
		<input type="email" 
				class="formfield single help Organisation Name with-placeholder"
				name="CoverLetter.Addressee.Organisation.Name"
				data-help-key="CoverLetter.Addressee.Organisation"
				value="{{CoverLetter.Addressee.Organisation.Name}}">
		<span name="CoverLetter.Addressee.Organisation.Name" class="help placeholder" style="display:none;">
			[[#resources]][[& EditorPlaceholder_CoverLetter:Addressee:Organisation]][[/resources]]
		</span>
	</fieldset>

	<fieldset class="CoverLetter.Addressee.ContactInfo.Address" name="Address">
